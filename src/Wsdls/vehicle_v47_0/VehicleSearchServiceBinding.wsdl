<?xml version="1.0" encoding="UTF-8"?>

<definitions name="VehicleServices"
             xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:xsd="http://www.w3.org/2001/XMLSchema"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:tns="http://www.travelport.com/service/vehicle_v47_0"
             xmlns:ns1="http://www.travelport.com/schema/vehicle_v47_0"
             targetNamespace="http://www.travelport.com/service/vehicle_v47_0">

    <import namespace="http://www.travelport.com/service/vehicle_v47_0"
            location="VehicleAbstract.wsdl"/>

    <binding name="VehicleSearchServiceBinding" type="tns:VehicleSearchServicePortType">
        <soap:binding style="document"
                      transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="service">
            <soap:operation soapAction="http://localhost:8080/kestrel/VehicleService"/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
            <fault name="ErrorInfoMsg">
                <soap:fault name="ErrorInfoMsg" use="literal"/>
            </fault>
        </operation>
    </binding><service name="VehicleService"><port name="VehicleSearchServicePort" binding="tns:VehicleSearchServiceBinding">
            <soap:address location="http://localhost:8080/kestrel/VehicleService"/>
        </port></service></definitions>